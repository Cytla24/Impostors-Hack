		"start": "yarn build && yarn backend:server"
	},
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"@googlemaps/google-maps-services-js": "^3.1.13",
		"axios": "^0.20.0",
		"concurrently": "4.1.0"
	},
	"devDependencies": {}
}
