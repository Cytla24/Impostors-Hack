{
	"name": "hackathon",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"yarn backend:server\" \"yarn frontend:server:dev\"",
		"frontend:server:dev": "yarn workspace client new_start",
